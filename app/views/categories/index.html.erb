<h1>Categories#index</h1>

<% @categories.each do |category| %>
<p><%= category.id %> <%= category.symptom %> <%= category.level %> <%= category.icon %>  <%= link_to "Edit", edit_category_path(category) %> <%= link_to 'Delete', category, method: :delete, data: { confirm: 'Are you sure?' } %></p>
<% end %>

